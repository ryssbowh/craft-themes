{% extends "fields/_field" %}

{% set show_field = options.viewMode is not null %}

{% block content %}
    <div class="slick-carousel" data-slick="{{ options.getAttributes(null, ['viewModeUid'])|json_encode }}">
        {% for tag in value %}
            {{ craft.themes.view.renderLayout(displayer.tagLayout, options.viewMode.handle, tag) }}
        {% endfor %}
    </div>
{% endblock %}